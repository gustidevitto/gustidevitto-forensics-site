# Google Sheets API Configuration
# Copy this file to .env.local and fill in your values

# Google Apps Script Web App URL
# Get this from: Google Sheets → Extensions → Apps Script → Deploy → Web app URL
VITE_GOOGLE_SHEETS_URL=https://script.google.com/macros/s/YOUR_DEPLOYMENT_ID/exec

# Optional: Enable/disable lead capture (for testing)
VITE_ENABLE_LEAD_CAPTURE=true
